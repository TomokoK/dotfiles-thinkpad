!! GENERAL
URxvt.scrollBar: false
URxvt.saveLines: 8192
URxvt.internalBorder: 15
URxvt.cursorUnderline: false
URxvt.cursorBlink: true
URxvt.allowBold: false

!! Enabling clickable links.
URxvt.perl-ext-common:  default,matcher
URxvt.matcher.button:   1
URxvt.url-launcher:     /usr/bin/firefox

!! FONT
urxvt*font: xft:Tewi:pixelsize=9,xft:JackeyFont:pixelsize=9,

urxvt*boldFont: xft:Tewi:pixelsize=9,xft:JackeyFont:pixelsize=9,

!! COLORS

! special
*.foreground: #becde7
*.background: #353945
*.cursorColor: #d6e0e8
*.borderColor: #353945

! black
*.color0: #9ca7a8
*.color8: #cad8da

! red
*.color1: #c3c5c3
*.color9: #f4f7f4

! green
*.color2: #b2bcbe
*.color10: #e0edf0

! yellow
*.color3: #a4b1b6
*.color11: #d1e1e8

! blue
*.color4: #809aaa
*.color12: #a5c7dc

! magenta
*.color5: #8da2ab
*.color13: #b6d1dd

! cyan
*.color6: #7293aa
*.color14: #93bedc

! white
*.color7: #648baa
*.color15: #81b3dc
