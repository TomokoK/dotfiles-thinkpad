!! GENERAL
URxvt.scrollBar: false
URxvt.saveLines: 8192
URxvt.internalBorder: 15
URxvt.cursorUnderline: false
URxvt.cursorBlink: true
URxvt.allowBold: false

!! Enabling clickable links.
URxvt.perl-ext-common:  default,matcher
URxvt.matcher.button:   1
URxvt.url-launcher:     /usr/bin/firefox

!! FONT
urxvt*font: xft:lucy tewi:pixelsize=9,xft:Freetype JackeyFont:pixelsize=9,-wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1,

urxvt*boldFont: xft:lucy tewi:pixelsize=9,xft:Freetype JackeyFont:pixelsize=9,-wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1,

!! COLORS

! special
*.foreground: #DDCCBB
*.background: #151515
*.cursorColor: #999999
*.borderColor: #151515

! black
*.color0: #666666
*.color8: #999999

! red
*.color1: #C27B83
*.color9: #D47C87

! green
*.color2: #99CC66
*.color10: #CCFF99

! yellow
*.color3: #CC9966
*.color11: #FFCC99

! blue
*.color4: #6699CC
*.color12: #99CCFF

! magenta
*.color5: #9966CC
*.color13: #CC99FF

! cyan
*.color6: #BA3D4D
*.color14: #BA3D4D

! white
*.color7: #CCCCCC
*.color15: #FFFFFF
