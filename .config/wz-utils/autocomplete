# For zsh:
# autoload -U +X bashcompinit && bashcompinit

_rhisk_comp()
{
    _rhisk_commands=$(rhisk -c)

    local cur prev
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=( $(compgen -W "${_rhisk_commands}" -- ${cur}) )

    return 0
}

complete -o nospace -F _rhisk_comp rhisk
